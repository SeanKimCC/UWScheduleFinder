<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <!-- Please see: http://www.liquibase.org/manual/column -->

    <changeSet id="1.0" author="seankim">

        <createTable tableName="UW_EXAM_SCHEDULE">

            <column name="ID" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="COURSE" type="varchar(255)">
            </column>
            <column name="SECTION" type="varchar(30)">
            </column>
            <column name="DAY" type="varchar(255)">
            </column>
            <column name="DATE" type="varchar(255)">
            </column>
            <column name="START_TIME" type="varchar(255)">
            </column>
            <column name="END_TIME" type="varchar(255)">
            </column>
            <column name="NOTE" type="varchar(255)">
            </column>
        </createTable>

    </changeSet>
</databaseChangeLog>